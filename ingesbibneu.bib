@inproceedings{LloydChamb2007n6,
	Abstract = { Early proponents of computer-mediated communication (CMC) argued that equitable forms of communication could be achieved using online learning environments. However, subsequent feminist research into the use of CMC suggested that the same gender inequities found in face-to-face discussion in classrooms can also be observed in online educational forums. This paper examines the online discussion forum of a postgraduate course in order to investigate whether the interactions occurring there demonstrate the gendered characteristics noted elsewhere in communications research. Analysis of forum data utilised a mixed-method approach and initial findings were used to inform the selection of cases for interview regarding their experience of communicating in the forum. The paper considers possible social and educational reasons why this forum may differ from those previously studied, and suggests possible mitigating methods for achieving gender equity in this and similar contexts. },
	Address = {Vancouver, Canada},
	Author = {Elizabeth Lloyd and Dianne Chambers},
	Booktitle = {Proceedings of World Conference on Educational Media and Technology 2007},
