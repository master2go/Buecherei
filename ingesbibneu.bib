@inproceedings{Hartsell2004yc,
	Abstract = { One question that arises concerning online communication is whether the gender of the student has a contributing factor toward learning success.  Many have voiced that the Internet is a more neutral playing field for both men and women to enjoy, and thus, should be taken seriously to create these opportunities for engagement.  This paper reports upon a preliminary descriptive study that was conducted during the fall 2003 semester with an online course that involved heavy participation in discussion threads to examine whether gender differences existed in online communication. An overview of the study will be discussed first along with supporting literature.  The paper will then report upon the results of the study.  Finally, the study will discuss the implications and recommendations for online instructors who are teaching diversified student populations, particularly those courses that have an even distribution of women and men. },
	Address = {Washington, DC, USA},
	Author = {Taralynn Hartsell},
	Booktitle = {Proceedings of World Conference on E-Learning in Corporate, Government, Healthcare, and Higher Education 2004},
	Date-Added = {2015-05-20 15:55:42 +0000},
